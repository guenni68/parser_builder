<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../abnf.xsd">
  <rule id="cs_string">
    <cs_literal value="strings"/>
  </rule>
  <rule id="twoStrings">
    <cs_literal value="me"/>
    <cs_literal value="you"/>
  </rule>
  <rule id="caseInsensitive">
    <ci_literal value="me"/>
    <ci_literal value="you"/>
  </rule>
  <rule id="ignore1">
    <ci_literal value="do"/>
    <ignore>
      <ci_literal value="ignore"/>
    </ignore>
    <ci_literal value="this"/>
  </rule>
  <rule id="tagged1">
    <ci_literal value="this"/>
    <ci_literal value="is"/>
    <tag name="tag1">
      <ci_literal value="tagged"/>
    </tag>
  </rule>
  <rule id="replace1">
    <ci_literal value="you"/>
    <ci_literal value="should"/>
    <ci_literal value="replace"/>
    <replace value="that">
      <ci_literal value="this"/>
    </replace>
  </rule>
</grammar>
